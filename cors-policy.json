{
  "rules": [
    {
      "id": "AllowWebAppAccess",
      "allowed": {
        "origins": ["https://mcrchoward.org", "http://localhost:3000", "https://*.vercel.app"],
        "methods": ["GET", "HEAD", "PUT", "POST", "DELETE"],
        "headers": ["*"]
      },
      "exposeHeaders": ["ETag"],
      "maxAgeSeconds": 3000
    }
  ]
}
